{
  "rules": [
    {
      "rule_id": "SUB-001",
      "category": "Submission",
      "severity": "High",
      "description": "Menu submissions must be in Microsoft Word (.docx) format using the official template.",
      "details": {
        "allowed_format": "docx",
        "disallowed_formats": [
          "xlsx",
          "pdf",
          "jpg",
          "png",
          "txt"
        ]
      }
    },
    {
      "rule_id": "SUB-002",
      "category": "Submission",
      "severity": "High",
      "description": "Proposed edits must be highlighted in yellow.",
      "details": {
        "change_type": "edit",
        "required_format": "yellow_highlight"
      }
    },
    {
      "rule_id": "SUB-003",
      "category": "Submission",
      "severity": "High",
      "description": "Item removals must be marked with a strikethrough in red.",
      "details": {
        "change_type": "removal",
        "required_format": "red_strikethrough",
        "legacy_alternative_formats": [
          "red_highlight"
        ],
        "legacy_note": "For legacy/older submissions only, red highlighted text may indicate removals. When interpreting such documents, treat red highlight as a removal equivalent. All new submissions must use red strikethrough."
      }
    },
    {
      "rule_id": "FMT-001",
      "category": "Formatting",
      "severity": "High",
      "description": "The 'DISH NAME' or 'DRINK NAME' must be in Calibri (Body), 12pt, Bold, and Sentence Case.",
      "details": {
        "target_element": "item_name",
        "font": "Calibri (Body)",
        "size": 12,
        "style": "Bold",
        "case": "Sentence"
      }
    },
    {
      "rule_id": "FMT-002",
      "category": "Formatting",
      "severity": "Medium",
      "description": "The item 'DESCRIPTION' text must be in Calibri (Body), 12pt, and always lowercase.",
      "details": {
        "target_element": "item_description",
        "font": "Calibri (Body)",
        "size": 12,
        "case": "lowercase",
        "do_not_capitalize": [
          "australian",
          "roasted",
          "grilled",
          "seared",
          "shaved",
          "stuffed",
          "chilean",
          "serrano",
          "mezcal",
          "braised",
          "smoked",
          "fried",
          "baked",
          "steamed",
          "poached",
          "charred",
          "pickled",
          "candied",
          "whipped",
          "toasted",
          "infused"
        ],
        "note": "Adjectives and ingredient modifiers in descriptions must remain lowercase. Do not force capitalization on these words even if they appear at the start of a continuation line."
      }
    },
    {
      "rule_id": "FMT-003",
      "category": "Formatting",
      "severity": "Medium",
      "description": "The 'PRICE' must be in Calibri (Body), 12pt.",
      "details": {
        "target_element": "price",
        "font": "Calibri (Body)",
        "size": 12
      }
    },
    {
      "rule_id": "CNT-001",
      "category": "Content",
      "severity": "High",
      "description": "The official RSH Logo must be present on the first page.",
      "details": {
        "required_element": "rsh_logo_image",
        "location": "page_1"
      }
    },
    {
      "rule_id": "PRC-001",
      "category": "Pricing",
      "severity": "High",
      "description": "Prices must be whole numbers. Dollar signs ($) and cents (.00) are not allowed.",
      "details": {
        "format": "integer",
        "disallowed_characters": [
          "$",
          "."
        ]
      }
    },
    {
      "rule_id": "PRC-002",
      "category": "Pricing",
      "severity": "Medium",
      "description": "A vertical bar '|' must be used to separate two prices (e.g., glass/bottle).",
      "details": {
        "dual_price_separator": "|",
        "disallowed_separators": [
          "/"
        ]
      }
    },
    {
      "rule_id": "ALG-001",
      "category": "Allergens",
      "severity": "High",
      "description": "Allergen notations must be placed on the same line as the dish name, in superscript.",
      "details": {
        "placement": "item_name_line",
        "style": "superscript"
      }
    },
    {
      "rule_id": "ALG-002",
      "category": "Allergens",
      "severity": "High",
      "description": "Multiple allergen codes must be listed in alphabetical order, comma-separated with no spaces (e.g., D,G,N).",
      "details": {
        "order": "alphabetical",
        "separator": ",",
        "spacing": "none"
      }
    },
    {
      "rule_id": "ALG-003",
      "category": "Allergens",
      "severity": "High",
      "description": "A raw/undercooked food warning must be present at the bottom of culinary pages.",
      "details": {
        "required_text": "CONSUMING RAW OR UNDERCOOKED MEATS, POULTRY, SEAFOOD, OR EGGS MAY INCREASE YOUR RISK OF FOODBORNE ILLNESS",
        "location": "footer"
      }
    },
    {
      "rule_id": "ALG-004",
      "category": "Allergens",
      "severity": "Medium",
      "description": "Raw or undercooked items must be marked with a trailing asterisk (*).",
      "details": {
        "marker": "*",
        "placement": "suffix"
      }
    },
    {
      "rule_id": "VOC-001",
      "category": "Vocabulary",
      "severity": "Low",
      "description": "Specific spelling and vocabulary must be used (e.g., 'barbeque' not 'BBQ', 'aioli' not 'mayo').",
      "details": {
        "spellings": {
          "barbeque": [
            "bbq"
          ],
          "aioli": [
            "mayo"
          ],
          "Caesar": [
            "cesar",
            "ceasar"
          ],
          "chili": [
            "chile"
          ],
          "tartare": [
            "tartar"
          ],
          "mozzarella": [
            "mozarella"
          ],
          "parmesan": [
            "parmesian"
          ],
          "yuzu kosho": [
            "yuzukosho",
            "yuzu-kosho"
          ],
          "passion fruit": [
            "passionfruit"
          ]
        }
      }
    },
    {
      "rule_id": "VOC-002",
      "category": "Vocabulary",
      "severity": "Low",
      "description": "Redundant words like 'fresh', 'creamy', 'warm', 'juice', 'syrup' should be avoided in descriptions.",
      "details": {
        "avoid_words": [
          "creamy",
          "warm",
          "fresh",
          "pepper",
          "garnish",
          "mesclun",
          "juice",
          "pur\u00e9e",
          "syrup"
        ]
      }
    },
    {
      "rule_id": "STR-001",
      "category": "Structure",
      "severity": "Medium",
      "description": "Menu categories (e.g., Appetizers, Entr\u00e9es) must be plural.",
      "details": {
        "target": "category_headings",
        "form": "plural"
      }
    },
    {
      "rule_id": "STR-002",
      "category": "Structure",
      "severity": "Medium",
      "description": "Ingredient descriptions must be singular, with specific exceptions.",
      "details": {
        "target": "ingredient_descriptors",
        "form": "singular",
        "exceptions": [
          "beans",
          "chips",
          "greens",
          "berries",
          "fries",
          "seeds",
          "herbs",
          "noodles",
          "sprouts",
          "diced vegetables",
          "mixed cheeses",
          "mixed mushrooms"
        ]
      }
    },
    {
      "rule_id": "STR-003",
      "category": "Structure",
      "severity": "Medium",
      "description": "Latin concepts must use specified Spanish category names.",
      "details": {
        "condition": "latin_concept",
        "required_categories": [
          "PARA LA MESA",
          "ANTOJITOS",
          "SOPAS",
          "ESPECIALIDADES",
          "POSTRES",
          "C\u00d3CTELES"
        ]
      }
    },
