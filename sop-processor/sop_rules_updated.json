    },
    {
      "rule_id": "STR-004",
      "category": "Structure",
      "severity": "Medium",
      "description": "Prix fixe and tasting menus must have numbered courses. Each course section should be preceded by its course number (1, 2, 3, etc.).",
      "details": {
        "applies_to": [
          "prix fixe",
          "pre-fix",
          "tasting menu",
          "degustation",
          "chef's menu",
          "multi-course"
