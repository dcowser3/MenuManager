    {
      "rule_id": "PUNC-001",
      "category": "Punctuation",
      "severity": "Low",
      "description": "Preserve existing ingredient separators. Do not change commas to slashes or split compound words.",
      "details": {
        "preserve_existing_separators": true,
        "do_not_split_compound_words": [
          "yuzu-lime",
          "cacao-ancho",
          "cucumber-cilantro",
          "huitlacoche-stuffed",
          "rose-petal",
          "house-made"
        ],
        "note": "The redliner should NOT automatically change ingredient separators. Compound words with hyphens should be preserved."
      }
    },
    {
      "rule_id": "FMT-008",
      "category": "Formatting",
      "severity": "Medium",
      "description": "Item names must not be ALL CAPS except for approved acronyms/brands.",
      "details": {
        "target_element": "item_name",
        "disallow_all_caps": true,
        "allowed_acronyms": [
          "RSH",
          "TT"
        ]
      }
    },
